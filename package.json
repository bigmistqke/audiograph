{
  "name": "audiograph",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "pnpm --filter @audiograph/app dev",
